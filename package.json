{
  "name": "rollup-plugin-generate-package-json",
  "description": "Generate package.json file with packages from your bundle using Rollup",
  "version": "2.1.1",
  "author": "Vlad Shcherbin <vlad.shcherbin@gmail.com>",
  "repository": "vladshcherbin/rollup-plugin-generate-package-json",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "lint": "eslint src tests",
    "test": "jest",
    "build": "rollup -c",
    "clean": "rimraf dist",
    "coverage": "yarn test --coverage && yarn codecov && yarn rimraf coverage",
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "postpublish": "yarn clean"
  },
  "dependencies": {
    "read-pkg": "^4.0.1",
    "write-pkg": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "codecov": "^3.1.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jest": "^24.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2"
  },
  "peerDependencies": {
    "rollup": ">=1.0.0"
  },
  "files": [
    "dist",
    "readme.md"
  ],
  "keywords": [
    "rollup",
    "rollup-plugin",
    "generate",
    "package.json"
  ],
  "engines": {
    "node": ">=8"
  },
