    "babel-jest": "^23.2.0",
    "codecov": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "jest": "^23.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^4.0.0-beta.7"
  },
  "files": [
    "dist",
    "readme.md"
  ],
  "keywords": [
    "rollup",
    "plugin",
    "generate",
    "package.json"
