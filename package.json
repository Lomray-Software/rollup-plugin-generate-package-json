  "dependencies": {
    "read-pkg": "^3.0.0",
    "write-pkg": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
